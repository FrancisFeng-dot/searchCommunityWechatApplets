*/

.modal-box {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0px;
  background: rgba(0, 0, 0, 0.6);
  overflow: hidden;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.modal-body {
  position: relative;
}

.modal-content {
  overflow: hidden;
  border-radius: 10rpx;
}

.add-group-dialog-main {
  width: 600rpx;
  height: 280rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 16rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.add-group-dialog-main>text {
  font-size: 26rpx;
  font-family: MicrosoftYaHei;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 34rpx;
}

.add-group-dialog-main>button {
  margin-top: 40rpx;
  width: 300rpx;
  height: 68rpx;
  background: rgba(16, 180, 252, 1);
  border-radius: 34rpx;
  font-size: 26rpx;
  font-family: MicrosoftYaHei;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  line-height: 70rpx;
}

.call-phone-dialog-main {
  width: 580rpx;
  height: 290rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 16rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
}

.call-dialog-title {
  font-size: 32rpx;
  font-family: SourceHanSansCN-Regular;
  font-weight: 400;
  color: rgba(26, 26, 26, 1);
  line-height: 44rpx;
  margin-top: 60rpx;
  margin-left: 50rpx;
  margin-right: 50rpx;
}

.call-phone-dialog-main>text {
  font-size: 28rpx;
  font-family: SourceHanSansCN-Regular;
  font-weight: 400;
  color: rgba(128, 128, 128, 1);
  line-height: 44rpx;
  margin-top: 26rpx;
  margin-left: 50rpx;
  margin-right: 50rpx;
}

.call-dialog-button-main {
  width: 100%;
  display: flex;
  flex-direction: row-reverse;
  justify-content: flex-start;
  margin-top: 20rpx;
}

.confirm-button {
  width: 100rpx;
  height: 60rpx;
  /* background: rgba(16, 180, 252, 1); */
  font-size: 32rpx;
  font-family: SourceHanSansCN-Regular;
  font-weight: 400;
  color: rgba(70, 180, 49, 1);
  line-height: 44rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  margin-left: 20rpx;
  margin-right: 50rpx;
}

.cancel-button {
  width: 100rpx;
  height: 60rpx;
  /* background: rgba(16, 180, 252, 1); */
  font-size: 32rpx;
  font-family: SourceHanSansCN-Regular;
  font-weight: 400;
  background: rgba(255, 255, 255, 1);
  line-height: 44rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.share-mian {
  width: 100%;
  background: #f7f7f7;
  position: fixed;
  bottom: 0rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.share-item-main {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.share-item-root {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background: rgba(255, 255, 255, 1);
  margin-bottom: 1rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border:0;
  border-radius: 0;
}

.share-item-root::after {
  border:0;
}

.share-item {
  height: 100rpx;
  background: rgba(255, 255, 255, 1);
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.share-item-logo {
  width: 48rpx;
  height: 48rpx;
}

.share-item-button {
  height: 100rpx;
  background: rgba(255, 255, 255, 1);
  font-size: 32rpx;
  font-family: MicrosoftYaHei;
  color: rgba(38, 28, 11, 1);
  text-align: center;
  line-height: 100rpx;
  border-radius: 0;
}

.share-item-button::after {
  border: none;
}

.share-item-line {
  width: 100%;
  height: 1rpx;
  background: #f7f7f7;
}

.share-dialog-close {
  width: 100%;
  margin-top: 10rpx;
  height: 100rpx;
  background: rgba(255, 255, 255, 1);
  font-size: 32rpx;
  font-family: MicrosoftYaHei;
  font-weight: 400;
  color: rgba(38, 28, 11, 1);
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
