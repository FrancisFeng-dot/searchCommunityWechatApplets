.group-bottom-rule {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.group-bottom-rule>text {
  font-size: 22rpx;
  font-family: MicrosoftYaHei;
  color: rgba(77, 77, 77, 1);
  margin-top: 10rpx;
}

.group-bottom-rule>image {
  width: 36rpx;
  height: 41rpx;
}

.group-edit-main {
  width: 100rpx;
  height: 100rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 5rpx 16rpx 0rpx rgba(0, 0, 0, 0.2);
  border-radius: 50%;
  position: fixed;
  bottom: 250rpx;
  right: 24rpx;
  background: white;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.group-edit-main>image {
  width: 34rpx;
  height: 37rpx;
}

.group-edit-main>text {
  font-size: 20rpx;
  font-family: MicrosoftYaHei;
  font-weight: 400;
  color: rgba(77, 77, 77, 1);
  margin-top: 5rpx;
}

